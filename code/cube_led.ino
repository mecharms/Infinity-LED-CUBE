void beat(){
  // colored stripes pulsing at a defined Beats-Per-Minute (BPM)
  uint8_t BeatsPerMinute = 45;
  CRGBPalette16 palette = PartyColors_p;
  uint8_t beat = beatsin8( BeatsPerMinute, 64, 255);
  for( int i = 0; i < 12; i++) { //9948
    leds_segments[i] = ColorFromPalette(palette, hue+(i*2), beat-hue+(i*10));
  }
   segments();
   FastLED.show();
    EVERY_N_MILLISECONDS(20){
   hue++;
  }
}


void confetti() {
  // random colored speckles that blink in and fade smoothly
  fadeToBlackBy( leds_buffor, 144, 10);
  int pos = random16(144);
  leds_buffor[pos] += CHSV( hue + random8(64), 200, 255);
  mirror();
   FastLED.show();
    EVERY_N_MILLISECONDS(20){
    hue++;
    }
}


void ColourSnake(){
  // eight colored dots, weaving in and out of sync with each other
  fadeToBlackBy( leds_buffor, 144, 20);
  uint8_t dothue = 0;
  for( int i = 0; i < 8; i++) {
    leds_buffor[beatsin16( i+1, 0, 143 )] |= CHSV(dothue, 200, 255);  // i+1 change to i+7 fot faster effect 
    dothue += 32;
  }
  mirror();
   FastLED.show();
    EVERY_N_MILLISECONDS(20){
    hue++;
}}

